    <attr name="title" format="string" />
    <attr name="description" format="string" />
    <attr name="subDescription" format="string" />

    <attr name="message" format="string" />
    <attr name="singleLine" format="boolean"/>
    <attr name="maxLength" format="integer" />

    <attr name="notEmpty" format="boolean" />

    <attr name="titleSize" format="dimension"/>
    <attr name="messageSize" format="dimension"/>

    <attr name="hint" format="string" />
    <attr name="subHint" format="string" />

    <attr name="isPassword" format="boolean"/>
    <attr name="keyboard" format="integer">
        <flag name="alphaNumeric" value="1"/>
        <flag name="alphaNumericComaAndDot" value="2"/>
        <flag name="numeric" value="3"/>
        <flag name="alphaNumericComaSlashAndDot" value="4"/>
        <flag name="numericAndSlash" value="5"/>
    </attr>

    <declare-styleable name="AutoFormatEditText">
        <attr name="isDecimal" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomCheckBox">
        <attr name="text" format="string"/>
        <attr name="checkboxBackground" format="reference"/>
        <attr name="textColor" format="color"/>
        <attr name="messageTypeface" format="integer">
            <flag name="bold" value="1"/>
            <flag name="italic" value="2"/>
            <flag name="normal" value="3"/>
        </attr>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomCardImageView">
        <attr name="title"/>
        <attr name="description"/>
    </declare-styleable>

    <declare-styleable name="FloatingEditText">
        <attr name="hint" />
        <attr name="subHint" />
        <attr name="singleLine"/>
        <attr name="maxLength" />
        <attr name="messageSize" />
        <attr name="message" />
        <attr name="isPassword" />
        <attr name="keyboard"/>
        <attr name="notEmpty"/>
        <attr name="customHintTextAppearance" format="reference"/>
        <attr name="customErrorTextAppearance" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FloatingMapsAutoCompleteText">
        <attr name="hint" />
        <attr name="subHint" />
        <attr name="singleLine"/>
        <attr name="maxLength" />
        <attr name="messageSize" />
        <attr name="message" />
        <attr name="isPassword"/>
        <attr name="keyboard"/>
        <attr name="notEmpty"/>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="title" />
        <attr name="message" />
        <attr name="titleColor" format="color"/>
        <attr name="titleSize" />
        <attr name="messageSize" />
        <attr name="notEmpty"/>
        <attr name="imageViewVisibility" format="integer">
            <flag name="gone" value="1"/>
            <flag name="invisible" value="2"/>
            <flag name="visible" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTextViewWithButtonText">
        <attr name="title"/>
        <attr name="singleLine"/>
        <attr name="maxLines" format="integer" />
        <attr name="maxLength"/>
        <attr name="inputType" format="enum" />
        <attr name="imeOptions" format="enum" />
        <attr name="enableDoneKeyboard" format="boolean" />
        <attr name="alphaNumeric" format="boolean" />
        <attr name="alphaNumericComaAndDot" format="boolean" />
        <attr name="numeric" format="boolean" />
        <attr name="phoneNumber" format="boolean" />
        <attr name="message"/>
        <attr name="disableText" format="boolean" />
        <attr name="enableWhenEdit" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomRadioProductView">
        <attr name="title"/>
        <attr name="description"/>
        <attr name="subDescription"/>
    </declare-styleable>

    <declare-styleable name="CustomRadioProductFacilityView">
        <attr name="title"/>
        <attr name="description1" format="string" />
        <attr name="description2" format="string" />
        <attr name="description3" format="string" />
        <attr name="otherInformation" format="string" />
        <attr name="contentImage" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ConfirmationHeaderSectionCustomView">
        <attr name="title"/>
        <attr name="textButton" format="string"/>
        <attr name="isHeaderVisible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="hint" />
        <attr name="subHint" />
        <attr name="error" format="string" />
        <attr name="size" format="integer" />
        <attr name="singleLine" />
        <attr name="keyboard" />
        <attr name="isPassword" />
        <attr name="maxLength"/>
        <attr name="notEmpty"/>
        <attr name="messageSize" />
        <attr name="minLength" format="integer" />
        <attr name="message" />
        <attr name="titleColor"/>
        <attr name="hintColor" format="integer|color"/>
        <attr name="editedTextColor" format="integer|color"/>
        <attr name="backgroundColor" format="integer|color"/>
        <attr name="isRounded" format="integer">
            <flag name="roundedAll" value="1"/>
            <flag name="roundedTop" value="2"/>
            <flag name="roundedBottom" value="3"/>
        </attr>
        <attr name="hintApperanceWithError" format="integer" />
        <attr name="hintApperanceWithoutError" format="integer" />
        <attr name="subHintApperance" format="integer" />
        <attr name="errorApperance" format="integer" />
    </declare-styleable>

    <declare-styleable name="BottomNavigationBar">
        <attr name="menu" format="reference"/>
        <attr name="menuBackgroundColor" format="integer|color"/>
        <attr name="menuItemTextColor" format="integer|color"/>
        <attr name="highlightMenuPosition" format="integer"/>
    </declare-styleable>
